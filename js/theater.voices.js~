var theater = new TheaterJS();
                   
theater.describe("Vader", .8, "#theater");
theater
 .write("Vader:... Imagine a web with no bottlenecks... ", 500)
 .write("Where content instead of being distributed from servers to clients ", 1000)
 .write("is shared directly between users. Without mediation.", 1000)
 .write(" Today this is possible, the web just got bigger.", 500);
